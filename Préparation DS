let Animals = [

    {name: 'Butters', age: 3, type: 'ğŸ¶'},

    {name: 'Lizzy',  age: 6, type: 'ğŸ¶'}, 

    {name: 'Minou', age: 10, type: 'ğŸ±'},

 {name: 'Butt', age: 33, type: 'ğŸ¶'},

    {name: 'Liza',  age: 62, type: 'ğŸ¶'}, 

    {name: 'Mina', age: 100, type: 'ğŸ±'},


];


function dog(v) {
if(v.type==='ğŸ¶')
return true


}

function add(prev,curr,i,t){
  
  let finale = prev + curr;
  return finale;
}


function fois(v){
    let tempAge =v.age;
    let k = tempAge *7;
    return k;
}


var valeurInitiale = 0;

let ok = Animals.filter(dog).map(fois).reduce(add);



console.log(`L'age  moyen des chiens est de ${ok}`);
